<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:mapbox="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout5"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".passenger.fragments.PassengerHome">

    <com.mapbox.maps.MapView
        android:id="@+id/map_passenger_home"
        android:layout_width="0dp"
        android:layout_height="0dp"
        mapbox:layout_constraintBottom_toTopOf="@+id/passenger_home_fragment_frame_home"
        mapbox:layout_constraintEnd_toEndOf="@+id/passenger_home_fragment_frame_home"
        mapbox:layout_constraintStart_toStartOf="parent"
        mapbox:layout_constraintTop_toBottomOf="parent"
        mapbox:layout_constraintTop_toTopOf="parent"
        mapbox:mapbox_cameraTargetLat="45.256930"
        mapbox:mapbox_cameraTargetLng="19.810578"
        mapbox:mapbox_cameraZoom="9.0">

    </com.mapbox.maps.MapView>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/passenger_home_fragment_frame_home"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:padding="0dp"
        mapbox:layout_constraintBottom_toBottomOf="parent"
        mapbox:layout_constraintEnd_toEndOf="parent"
        mapbox:layout_constraintStart_toStartOf="parent"
        tools:layout="@layout/fragment_blank" />
</androidx.constraintlayout.widget.ConstraintLayout>